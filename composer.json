{
    "name": "swoft/devtool",
    "type": "library",
    "keywords": [
        "php",
        "swoole",
        "swoft",
        "debug",
        "devtool"
    ],
    "description": "Devtool for swoft framework",
    "license": "Apache-2.0",
    "require": {
    },
    "autoload": {
        "psr-4": {
            "Swoft\\Devtool\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SwoftTest\\Devtool\\": "test/case"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packagist.laravel-china.org"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^7.5"
    },
    "scripts": {
        "test": "./vendor/bin/phpunit",
        "post-package-update": "echo 'Please rerun command: php bin/swoft dev:publish swoft/devtool -f'"
    }
}
